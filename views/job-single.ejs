                                          <button type="submit" class="theme-btn btn-style-one">Upload Resume</button>
                                          <button type="button" class="theme-btn btn-style-one ml-2" onclick="closeModal()">Cancel</button>
                                      </div>
                                  </form>
                              </div>
                          </div>
                      </div>
